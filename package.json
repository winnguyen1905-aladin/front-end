{
  "name": "front-end",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "pm2:dev": "pm2 start ecosystem.config.cjs --only aladin-frontend-dev",
    "pm2:prod": "pm2 start ecosystem.config.cjs --only aladin-frontend-prod",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs aladin-frontend-prod",
    "pm2:monit": "pm2 monit"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.2.2",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "mediasoup-client": "^3.7.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "serve": "^14.2.1",
    "socket.io-client": "^4.8.1"
  }
}
